{
    "manifest_version": 3,
    "name": "Kibana Extender",
    "version": "1.0",
    "description": "A Chrome extension to manage a list of search entries.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": [
      "storage",
      "tabs",
      "activeTab"
    ]
  }
  